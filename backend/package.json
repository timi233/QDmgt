{
  "name": "channel-backend",
  "version": "1.0.0",
  "private": true,
  "description": "渠道管理系统后端 API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --noEmitOnError false || true",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=integration",
    "test:performance": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=performance",
    "backup:create": "tsx src/scripts/backup.ts create",
    "backup:list": "tsx src/scripts/backup.ts list",
    "backup:restore": "tsx src/scripts/backup.ts restore",
    "backup:verify": "tsx src/scripts/backup.ts verify",
    "backup:stats": "tsx src/scripts/backup.ts stats"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@prisma/client": "^5.7.0",
    "@types/bcryptjs": "^2.4.6",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
